<h2>住所情報登録</h2>

<%= form_for @user_address do |f| %>
  <%= render "devise/shared/error_messages", resource: @user_address %>

  <div class="field">
    <%= f.label :user_postal_code %><br />
    <%= f.text_field :user_postal_code %>
  </div>

  <div class="field">
    <%= f.label :user_addresses %><br />
    <%= f.text_field :user_addresses %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>